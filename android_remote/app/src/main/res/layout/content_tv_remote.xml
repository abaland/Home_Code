<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:context="com.example.abaland.android_remote.MainActivity">

    <Button
            android:text="@string/up"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/up"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/power" android:layout_alignLeft="@+id/ok" android:layout_alignStart="@+id/ok"/>
    <Button
            android:text="@string/mode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mode"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/power" android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"/>
    <Button
            android:text="@string/right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/right"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/power" android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" android:layout_marginTop="47dp"/>
    <Button
            android:text="@string/back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/back"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/power" android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"/>
    <Button
            android:text="@string/left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/left"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_alignBaseline="@+id/right" android:layout_alignBottom="@+id/right"
            android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
    <Button
            android:text="@string/ok"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ok"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_alignBaseline="@+id/right" android:layout_alignBottom="@+id/right" android:layout_centerHorizontal="true"/>
    <Button
            android:text="@string/bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/bottom"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/ok" android:layout_alignLeft="@+id/ok" android:layout_alignStart="@+id/ok"/>
    <Button
            android:text="@string/button1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button1"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/bottom" android:layout_alignRight="@+id/left" android:layout_alignEnd="@+id/left"/>
    <Button
            android:text="@string/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button2"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_alignBottom="@+id/button1" android:layout_alignRight="@+id/bottom" android:layout_alignEnd="@+id/bottom"/>
    <Button
            android:text="@string/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button3"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_alignBottom="@+id/button2" android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
    <Button
            android:text="@string/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button4"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/button1" android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
    <Button
            android:text="@string/button5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button5"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/button2" android:layout_alignLeft="@+id/button2" android:layout_alignStart="@+id/button2"/>
    <Button
            android:text="@string/button6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button6"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_alignBottom="@+id/button5" android:layout_alignParentRight="true" android:layout_alignParentEnd="true" />
    <Button
            android:text="@string/button7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button7"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/button4" android:layout_alignRight="@+id/button4" android:layout_alignEnd="@+id/button4"/>
    <Button
            android:text="@string/button8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button8"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_alignBottom="@+id/button7" android:layout_alignLeft="@+id/button5" android:layout_alignStart="@+id/button5"/>
    <Button
            android:text="@string/button9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button9"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/button6" android:layout_alignParentRight="true" android:layout_alignParentEnd="true" />
    <Button
            android:text="@string/channelup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/channelup"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_alignTop="@+id/channeldown" android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
    <Button
            android:text="@string/volumeup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/volumeup"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/channelup" android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
    <Button
            android:text="@string/channeldown"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/channeldown"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/button7" android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
    <Button
            android:text="@string/volumedown"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/volumedown"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/channeldown" android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"/>
    <Button
            android:text="@string/subs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/subs"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/left" android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"/>
    <Button
            android:text="@string/mute"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mute"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_below="@+id/right" android:layout_alignLeft="@+id/button3" android:layout_alignStart="@+id/button3"/>
    <Button
            android:text="@string/power"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/power"
            android:onClick="onClickFunction" tools:ignore="UnknownId"
            android:layout_marginTop="77dp"
            android:layout_alignParentTop="true" android:layout_alignParentStart="true"/>
</RelativeLayout>
